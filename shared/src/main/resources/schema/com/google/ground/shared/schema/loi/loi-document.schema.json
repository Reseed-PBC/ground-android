{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "schema:loi/loi-document",
  "title": "Location of Interest",
  "type": "object",
  "properties": {
    "jobId": {
      "type": "string"
    },
    "customId": {
      "type": "string"
    },
    "geometry": {
      "$ref": "schema:geometry/nested-geometry"
    },
    "properties": {
      "type": "object",
      "x-kt-type": "map"
    },
    "submissionCount": {
      "type": "integer"
    }
  },
  "required": ["jobId", "geometry"]
}
